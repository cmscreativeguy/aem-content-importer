<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <entry key="source-format">DITA</entry>
    <entry key="master-file">mcloud.ditamap</entry>
    <entry key="graphics-folders">images,graphics,Graphics</entry>
    <entry key="destination">/content/aem-content-importer/imported</entry>

    <entry key="xml-transformer">com.adobe.aem.importer.impl.XMLTransformerDITAImpl</entry>
    <entry key="src">/var/aem-importer/zip</entry>
    <entry key="xslt-transformer">net.sf.saxon.TransformerFactoryImpl</entry>
    <entry key="tempFolder">/var/aem-importer/tmp</entry>
    <entry key="packageTpl">/apps/aem-importer/resources/package-tpl</entry>
</properties>
