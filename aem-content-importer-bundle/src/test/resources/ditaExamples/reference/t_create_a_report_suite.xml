<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE task PUBLIC "-//OASIS//DTD DITA Task//EN" "task.dtd">
<task id="task_67033B9710CB49F9B71A4DE374A538A0"> 
  <title>Create a report suite</title> 
  <shortdesc>Steps that describe how to create a report suite. 
  </shortdesc> 
  <prolog> 
	 <metadata> 
		<othermeta name="solution" content="Analytics"/> 
		<othermeta name="topic" content="Admin tools"/> </metadata>
  </prolog> 
  <taskbody> 
	 <steps id="steps_F369CFAF25E3411E987E29FB890C13BD"> 
		<step id="step_A9BD22CCE627436A8481848C370F28CF"> 
		  <cmd>Click 
			 <uicontrol>Analytics</uicontrol> &gt; 
			 <uicontrol>Admin</uicontrol> &gt; 
			 <uicontrol>Report Suites</uicontrol>. 
		  </cmd> 
		</step> 
		<step id="step_B2AEB72CBAB64036BE2ED419CFDC13B6"> 
		  <cmd>Select a report suite. 
		  </cmd> 
		</step> 
		<step id="step_557FB96DC569491C922EF07C46D9C006"> 
		  <cmd>Click 
			 <uicontrol>Create New</uicontrol> &gt; 
			 <uicontrol>Report Suite</uicontrol>. 
		  </cmd> 
		</step> 
		<step id="step_397EF48499734950B2C4FB1881C2B1E5"> 
		  <cmd>In the template list, select either a predefined template or an
			 existing report suite to use as a 
			 <xref
			  href="report_suite_templates.xml#concept_539B8C5468424D31B55FC6EB39C5061F"
			  format="dita" scope="local">template</xref>. 
		  </cmd> 
		</step> 
		<step id="step_3370DA349C6542B8A6B1E3A1B51FEF7B"> 
		  <cmd>Fill in the fields described in 
			 <xref
			  href="new_report_suite.xml#concept_3CBBE252279C43069030EFBBD7D26376"
			  format="dita" scope="local"><?xm-replace_text New Report Suite?></xref>. 
		  </cmd> 
		</step> 
		<step id="step_DD2F20129FA6489D92B588AFAA4894F4"> 
		  <cmd>Click 
			 <uicontrol>Create Report Suite</uicontrol>. 
		  </cmd> 
		</step> 
	 </steps> 
  </taskbody> 
</task>
