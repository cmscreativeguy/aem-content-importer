<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE task PUBLIC "-//OASIS//DTD DITA Task//EN" "task.dtd">
<task id="task_4BC14D46152F4DED97D5B2BCE6520B00"> 
  <title>Set up the custom calendar</title> 
  <shortdesc>Steps that describe how to set up the custom calendar. 
  </shortdesc> 
  <prolog> 
	 <metadata> 
		<othermeta name="solution" content="Analytics"/> 
		<othermeta name="topic" content="Admin tools"/> </metadata>
  </prolog> 
  <taskbody> 
	 <steps id="steps_E94FD0C1AAA34EBCA9448A79C2C0C346"> 
		<step id="step_96CB3DD8012E4FB493C76ABEE295ABB5"> 
		  <cmd>Click 
			 <uicontrol>Analytics</uicontrol> &gt; 
			 <uicontrol>Admin</uicontrol> &gt; 
			 <uicontrol>Report Suites</uicontrol>. 
		  </cmd> 
		</step> 
		<step id="step_B2AEB72CBAB64036BE2ED419CFDC13B6"> 
		  <cmd>On the 
			 <wintitle>Report Suite Manager</wintitle>, select a report suite. 
		  </cmd> 
		</step> 
		<step id="step_A326345ED8BE49738A22657FB55ADF8E"> 
		  <cmd>Click 
			 <uicontrol>Edit Settings</uicontrol> &gt; 
			 <uicontrol>General</uicontrol> &gt; 
			 <uicontrol>Customize Calendar</uicontrol>. 
		  </cmd> 
		</step> 
		<step id="step_182BF4F56588412F9A42367D4F7B568D"> 
		  <cmd>Select a calendar type described in 
			 <xref
			  href="custom_calendar.xml#concept_4342A844600048759EEDABD164AC3F5A/section_B0D224DACB914A378902A4E0E1234889"
			  format="dita" scope="local"><?xm-replace_text Customize Calendar Definitions?></xref>.
			 
		  </cmd> 
		</step> 
		<step id="step_47E0425C1E924B77B6159D2B7CF220FD"> 
		  <cmd>Click 
			 <uicontrol>Save</uicontrol>. 
		  </cmd> 
		</step> 
	 </steps> 
  </taskbody> 
</task>
