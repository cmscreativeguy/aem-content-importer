<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE task PUBLIC "-//OASIS//DTD DITA Task//EN" "task.dtd"[]>
<task id="task_B7A78FBBAB814B13ACF3AB105D4C2EEE">
  <title>Export classification data using the browser</title>
  <shortdesc>Steps that describe how to download a data set via a web browser. 
  </shortdesc>
  <prolog>
    <metadata>
      <othermeta name="solution" content="Analytics" />
      <othermeta name="topic" content="Admin tools" />
      <othermeta name="subtopic" content="Classifications" />
    </metadata>
  </prolog>
  <taskbody>
    <steps id="steps_0FC66AB8CEBE45AAB3F15F242F01B6EC">
      <step id="step_6F8ECD6EA1D74DBA834949FE7D30202E">
        <cmd>Click 
			 <uicontrol>Admin Tools</uicontrol> &gt; 
			 <uicontrol>Classification Importer</uicontrol>. 
		  </cmd>
      </step>
      <step id="step_3539B04805984A35A0D903A174DF69A1">
        <cmd>Click 
			 <uicontrol>Browser Export</uicontrol>. 
		  </cmd>
      </step>
      <step id="step_841F0B7DDB16423AA8B31E3A550E638B">
        <cmd>Specify the 
			 <xref href="browser_export.xml#concept_3D930F03840A491D8B37FAAE9C90F5DF" format="dita" scope="local">data set details</xref>. 
		  </cmd>
      </step>
      <step id="step_5EEBBD22064F4C45ADBB1F84670D57E7">
        <cmd>Click 
			 <uicontrol>Export File</uicontrol>. 
		  </cmd>
      </step>
      <step id="step_D9F8EEC43A4B4DE0B3DA468E2DB04B81">
        <cmd>Save the data set to your local system. 
		  </cmd>
        <info></info>
      </step>
    </steps>
  </taskbody>
</task>