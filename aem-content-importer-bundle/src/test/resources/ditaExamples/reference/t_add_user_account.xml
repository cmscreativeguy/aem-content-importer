<task id="task_60F86F36CB86446699EA7C7E5FB03EA7"> 
  <title>Add a user account</title> 
  <shortdesc>Steps that describe how to add a user account. 
  </shortdesc> 
  <prolog> 
	 <metadata> 
		<othermeta name="solution" content="Analytics"/> 
		<othermeta name="topic" content="Admin tools"/> 
		<othermeta name="subtopic" content="Users and groups"/> </metadata>
  </prolog> 
  <taskbody> 
	 <steps id="steps_5C2FCE719DDD4DD980CA321272ECA88B"> 
		<step id="step_404B55BF499B4C3F99D25D5178503A23"> 
		  <cmd> 
			 <uicontrol>Analytics</uicontrol> &gt; 
			 <uicontrol>Admin</uicontrol> &gt; 
			 <uicontrol>User Management</uicontrol>. 
		  </cmd> 
		</step> 
		<step id="step_23D0207A1ABA4DE2A72F06EE248E03E0"> 
		  <cmd>Click 
			 <uicontrol>Add New User</uicontrol>. 
		  </cmd> 
		</step> 
		<step id="step_BEE6968FBE384B67A4AAD33E81F72C55"> 
		  <cmd>Complete the fields specified in 
			 <xref
			  href="users.xml#concept_3B50D355B0AA48A58CFAB671CE3F82F4/section_14A7E169514A42A88E06387CC7C2E9AD"
			  format="dita" scope="local"><?xm-replace_text User Account Descriptions?></xref>.
			 
		  </cmd> 
		</step> 
	 </steps> 
  </taskbody> 
</task>
