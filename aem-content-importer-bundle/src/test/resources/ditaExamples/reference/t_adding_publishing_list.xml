<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE task PUBLIC "-//OASIS//DTD DITA Task//EN" "task.dtd">
<task id="task_9B5DC5570EBF4449B16E842748BA08B1"> 
  <title>Add a publishing list</title> 
  <shortdesc>Steps that describe how to create a publishing list for the
	 company. 
  </shortdesc> 
  <prolog> 
	 <metadata> 
		<othermeta name="solution" content="Analytics"/> 
		<othermeta name="topic" content="Admin tools"/> </metadata>
  </prolog> 
  <taskbody> 
	 <steps id="steps_54E2B290A09D4FC08A210DC069674E3F"> 
		<step id="step_404B55BF499B4C3F99D25D5178503A23"> 
		  <cmd>Click 
			 <uicontrol>Analytics</uicontrol> &gt; 
			 <uicontrol>Admin</uicontrol> &gt; 
			 <uicontrol>User Management</uicontrol>. 
		  </cmd> 
		</step> 
		<step id="step_1255EA1A9FB941DFB1AF601523766708"> 
		  <cmd>Click 
			 <uicontrol>Publishing List Manager</uicontrol>. 
		  </cmd> 
		</step> 
		<step id="step_23D0207A1ABA4DE2A72F06EE248E03E0"> 
		  <cmd>Click 
			 <uicontrol>Add Publishing List</uicontrol>. 
		  </cmd> 
		</step> 
		<step id="step_28E20DFDF6B44BB5907A25509485BB57"> 
		  <cmd>In the 
			 <uicontrol>Publishing List Name</uicontrol> field, specify the
			 publishing list name. 
		  </cmd> 
		</step> 
		<step id="step_939D8C8EB5934422A66DF5DF23AC683B"> 
		  <cmd>Configure the options described in 
			 <xref
			  href="publishing_list.xml#concept_D7939363573A457F9279FEDFB4A24BF7/section_099DF8AC5691495F9B22C71266DD6004"
			  format="dita" scope="local"><?xm-replace_text Publishing List Manager Descriptions?></xref>.
			 
		  </cmd> 
		</step> 
		<step id="step_5B427B75B24E4ABF8AE8360EE56A3F65"> 
		  <cmd>Repeat the previous step for each report suite you want to include
			 in the publishing list. 
		  </cmd> 
		</step> 
		<step id="step_5FAE48B640804C56B620CA36B8D60C39"> 
		  <cmd>Click 
			 <uicontrol>Save</uicontrol>. 
		  </cmd> 
		</step> 
	 </steps> 
  </taskbody> 
</task>
