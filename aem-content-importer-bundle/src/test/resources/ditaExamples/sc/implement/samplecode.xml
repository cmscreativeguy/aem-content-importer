        <colspec colnum="1" colname="col1" colwidth="1.00*" />
        <colspec colnum="2" colname="col2" colwidth="2.39*" />
        <thead>
          <row>
            <entry> Variable 
				</entry>
            <entry> Value 
				</entry>
          </row>
        </thead>
        <tbody>
          <row>
            <entry> pageName 
				</entry>
            <entry> Order Confirmation 
				</entry>
          </row>
          <row>
            <entry> Current URL 
				</entry>
            <entry> https://www.somesite.com/cart/confirmation.asp 
				</entry>
          </row>
          <row>
            <entry> events 
				</entry>
            <entry> purchase,event1 
				</entry>
          </row>
          <row>
            <entry> c1 
				</entry>
            <entry> Registered 
