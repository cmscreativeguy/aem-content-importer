<concept id="concept_19E6C850C3244CB6973140709BDCB0B9">
  <title>mediaSession</title>
  <shortdesc>This variable specifies the segments of a video or media asset
	 consumed. 
  </shortdesc>
  <prolog>
    <metadata>
      <othermeta name="solution" content="Analytics" />
      <othermeta name="topic" content="Developer and implementation" />
      <othermeta name="subtopic" content="Variables" />
    </metadata>
  </prolog>
