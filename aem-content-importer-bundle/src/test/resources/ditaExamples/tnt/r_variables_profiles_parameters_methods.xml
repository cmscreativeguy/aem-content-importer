	 <section id="section_8C78059D15D9452F95636A5640188537"> 
		<title>Methods</title> 
		<p> 
		<table id="table_0E99AD9702944F749BE37DCBAA34DD76"> 
		  <tgroup cols="2"> 
			 <colspec colnum="1" colname="col1" colwidth="*"/> 
			 <colspec colnum="2" colname="col2" colwidth="*"/> 
			 <thead> 
				<row> 
				  <entry colname="col1">Method 
				  </entry> 
				  <entry colname="col2">Notes 
				  </entry> 
				</row> 
			 </thead> 
			 <tbody> 
				<row> 
				  <entry colname="col1"> 
					 <p> 
						<codeph>mbox.getName() 
						</codeph> 
					 </p> 
				  </entry> 
				  <entry colname="col2">
				  </entry> 
				</row> 
				<row> 
				  <entry colname="col1"> 
					 <p> 
						<codeph>mbox.getURL() 
						</codeph> 
					 </p> 
				  </entry> 
				  <entry colname="col2">
				  </entry> 
				</row> 
				<row> 
				  <entry colname="col1"> 
					 <p> 
						<codeph>mbox.getDiv() 
						</codeph> 
					 </p> 
				  </entry> 
				  <entry colname="col2">Returns the 
					 <codeph>div</codeph> associated with the mbox (containing
					 default content or an offer) 
				  </entry> 
				</row> 
				<row> 
				  <entry colname="col1"> 
					 <p> 
						<codeph>mbox.getParameters() 
						</codeph> 
					 </p> 
				  </entry> 
				  <entry colname="col2">An array of parameters with two fields,
					 name and value 
				  </entry> 
				</row> 
				<row> 
				  <entry colname="col1"> 
					 <p> 
						<codeph>mbox.setOnload() 
						</codeph> 
					 </p> 
				  </entry> 
