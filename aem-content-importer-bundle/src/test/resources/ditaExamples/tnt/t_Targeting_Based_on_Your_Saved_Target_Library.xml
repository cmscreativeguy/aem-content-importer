<task id="task_F542F06596F94CEEBEF7025434737474">
  <title>Target Based on Your Saved Target Library</title>
  <shortdesc>You can target your campaign based on your saved target library. 
  </shortdesc>
  <prolog>
    <metadata>
      <othermeta name="solution" content="Target" />
      <othermeta name="topic" content="Advanced" />
    </metadata>
  </prolog>
  <taskbody>
    <steps id="steps_6789C304E3B04E5580F2C75C5478B4B2">
      <step id="step_A4A327988B2E43F89BFCEAA56EEE499E">
        <cmd>Click 
			 <uicontrol>Target this campaign</uicontrol>. 
		  </cmd>
      </step>
      <step id="step_05339A22323341ED97C7065BEF409233">
        <cmd>Click 
			 <uicontrol>Target Library</uicontrol>. 
		  </cmd>
      </step>
      <step id="step_9086CE3FEF71424AADB2FD338BD211CB">
        <cmd>Select the saved target you desire. 
		  </cmd>
      </step>
      <step id="step_D1B533CEC54649BAAF0A52571AADF800">
        <cmd>(Optional) Add additional conditions. 
		  </cmd>
      </step>
      <step id="step_AC0D920F284849BE92F1A657612EB0FC">
        <cmd>Click 
			 <uicontrol>Done</uicontrol>. 
		  </cmd>
        <stepresult>
          <?xm-replace_text Step Result?>
        </stepresult>
      </step>
    </steps>
  </taskbody>
